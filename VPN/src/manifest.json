{
    "manifest_version": 3,
    "name": "VPN", 
    "version": "1.0",
    "description": "Тут что-то должно быть, но этого нет",
    
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo16.png",
            "48": "logo48.png",
            "128": "logo128.png"
        }
    },
    
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },

    "permissions": [
        "proxy",
        "activeTab",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ]
}